issn = {1367-4803},
journal = {Bioinformatics (Oxford, England)},
keywords = {Algorithms,Computer Graphics,Computer Simulation,Data Interpretation, Statistical,Models, Biological,Models, Statistical,Protein Interaction Mapping,Protein Interaction Mapping: methods,Proteins,Proteins: metabolism,Signal Transduction,Signal Transduction: physiology,Statistical Distributions},
month = {apr},
