journal = {PLoS computational biology},
keywords = {Algorithms,Computational Biology,Computational Biology: methods,Databases, Protein,Humans,Models, Biological,Protein Interaction Mapping,Protein Interaction Mapping: methods,Proteins,Proteins: chemistry,Proteins: metabolism,ROC Curve,Reproducibility of Results,Sensitivity and Specificity,Signal Transduction},
month = {aug},
