doi = {10.1016/j.semcancer.2011.07.002},
file = {:home/rcannood/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/{\O}ra, Eggert - 2011 - Progress in treatment and risk stratification of neuroblastoma impact on future clinical and basic research.pdf:pdf},
issn = {1096-3650},
journal = {Seminars in cancer biology},
keywords = {Animals,Biomedical Research,Child,Humans,Neuroblastoma,Neuroblastoma: classification,Neuroblastoma: genetics,Neuroblastoma: therapy,Risk},
month = {oct},
