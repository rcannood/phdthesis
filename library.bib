pages = {699--704},
pmid = {10600714},
title = {{Gene and genome duplications in vertebrates: the one-to-four (-to-eight in fish) rule and the evolution of novel gene functions.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10600714},
volume = {11},
