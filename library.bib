issn = {1477-4054},
journal = {Briefings in bioinformatics},
keywords = {Algorithms,Animals,Cluster Analysis,Factor Analysis, Statistical,Gene Expression,Humans,Models, Theoretical,Oligonucleotide Array Sequence Analysis},
month = {may},
number = {3},
pages = {279--92},
pmid = {22772837},
title = {{A comparative analysis of biclustering algorithms for gene expression data.}},
