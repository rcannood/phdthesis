month = {jan},
number = {3},
pages = {e33624},
pmid = {22479422},
title = {{Bagging statistical network inference from large-scale gene expression data.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3316596{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {7},
year = {2012}
