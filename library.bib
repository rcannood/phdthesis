@article{Xu2015-kn,
abstract = {Upon recognition of antigen, B cells undertake a bifurcated
response in which some cells rapidly differentiate into
plasmablasts while others undergo affinity maturation in
germinal centers (GCs). Here we identified a double-negative
feedback loop between the transcription factors IRF4 and IRF8
that regulated the initial developmental bifurcation of
