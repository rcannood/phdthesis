journal = {BMC systems biology},
keywords = {Assessment of better gene clusters on bulk tissue,Brain,Cluster Analysis,Computational Biology,Databases,Gene Regulatory Networks,Gene co-expression networks on brain,Genetic,Humans,K-means applied to WGCNA,Software,Stochastic Processes,Transcriptome},
