issn = {1471-2105},
journal = {BMC bioinformatics},
keywords = {Algorithms,Cluster Analysis,Humans,Models, Biological,Protein Interaction Mapping,Protein Interaction Mapping: methods,Sequence Alignment,Software,Viral Proteins,Viral Proteins: metabolism},
month = {jan},
