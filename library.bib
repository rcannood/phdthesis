@article{Buganim2012-zl,
abstract = {During cellular reprogramming, only a small fraction of cells
become induced pluripotent stem cells (iPSCs). Previous
analyses of gene expression during reprogramming were based on
populations of cells, impeding single-cell level
identification of reprogramming events. We utilized two gene
expression technologies to profile 48 genes in single cells at
various stages during the reprogramming process. Analysis of
early stages revealed considerable variation in gene
expression between cells in contrast to late stages.
Expression of Esrrb, Utf1, Lin28, and Dppa2 is a better
predictor for cells to progress into iPSCs than expression of
the previously suggested reprogramming markers Fbxo15, Fgf4,
and Oct4. Stochastic gene expression early in reprogramming is
followed by a late hierarchical phase with Sox2 being the
upstream factor in a gene expression hierarchy. Finally,
downstream factors derived from the late phase, which do not
include Oct4, Sox2, Klf4, c-Myc, and Nanog, can activate the
pluripotency circuitry.},
author = {Buganim, Yosef and Faddah, Dina A and Cheng, Albert W and Itskovich, Elena and Markoulaki, Styliani and Ganz, Kibibi and Klemm, Sandy L and van Oudenaarden, Alexander and Jaenisch, Rudolf},
journal = {Cell},
number = {6},
pages = {1209--1222},
title = {{Single-cell expression analyses during cellular reprogramming reveal an early stochastic and a late hierarchic phase}},
volume = {150},
year = {2012}
}
