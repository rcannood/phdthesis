journal = {Mammalian genome : official journal of the International Mammalian Genome Society},
keywords = {Algorithms,Animals,Body Weight,Body Weight: genetics,Chromosomes, Mammalian,Crosses, Genetic,Gene Expression Profiling,Gene Expression Profiling: methods,Gene Regulatory Networks,Mice,Mice, Inbred C3H,Mice, Inbred C57BL,Mice, Inbred DBA,Quantitative Trait Loci},
