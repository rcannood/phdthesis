journal = {Genome research},
keywords = {Algorithms,BRCA1 Protein,BRCA1 Protein: genetics,Computational Biology,Computational Biology: methods,DNA Mutational Analysis,DNA Mutational Analysis: methods,DNA Mutational Analysis: standards,Female,Genes, Neoplasm,Humans,Molecular Sequence Annotation,Molecular Sequence Annotation: methods,Mutation,Ovarian Neoplasms,Ovarian Neoplasms: genetics,Reproducibility of Results,Software},
